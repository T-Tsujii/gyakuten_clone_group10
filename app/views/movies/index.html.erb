
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="members.scss" type="text/css">
</head>

<body>
  <% @movies.each do |movie| %>
    <div class="youtube-title"><%= movie.title %></div>
    <% iframe = content_tag(
      :iframe,
      '', # empty body
      width: 560,
      height: 315,
      src: movie.url,
      frameborder: 0,
      allowfullscreen: true
      ) %>
    <%= content_tag(:div, iframe, class: 'youtube-container') %>
  <% end %>

</body>
